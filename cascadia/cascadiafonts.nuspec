<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>{{packageName}}</id>
    <version>{{cascadiaVersion}}</version>
    <title>{{displayName}}</title>
    <authors>Microsoft</authors>
    <owners>zunderscore</owners>
    <projectUrl>{{cascadiaProjectRoot}}</projectUrl>
    <licenseUrl>{{cascadiaProjectRoot}}/blob/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <projectSourceUrl>{{cascadiaProjectRoot}}</projectSourceUrl>
    <packageSourceUrl>https://github.com/zunderscore/chocolatey-packages</packageSourceUrl>
    <tags>{{tags}}</tags>
    <summary>{{displayName}}</summary>
    <description>{{description}}</description>
    <dependencies>
      <dependency id="cascadiacode" version="{{cascadiaVersion}}" />
      <dependency id="cascadiacodepl" version="{{cascadiaVersion}}" />
      <dependency id="cascadiamono" version="{{cascadiaVersion}}" />
      <dependency id="cascadiamonopl" version="{{cascadiaVersion}}" />
    </dependencies>
    <releaseNotes>{{releaseNotes}}</releaseNotes>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
  </files>
</package>
